<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cam">
   <insert id="insertCoAndMa" statementType="PREPARED"
           parameterType="javaCoffe.spring.mvc.vo.CoAndMaVO">
      insert into cam(numlist, invname, kof)
      value (#{numlist},#{invname},#{kof})

   </insert>

</mapper>
