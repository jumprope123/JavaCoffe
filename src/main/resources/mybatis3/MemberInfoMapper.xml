<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="memberInfo">
    <select id="selectMemberInfo" statementType="PREPARED" parameterType="String"
            resultType="javaCoffe.spring.mvc.vo.MemberVO">
        select * from MemberJoin where userid = #{uid}
    </select>

</mapper>